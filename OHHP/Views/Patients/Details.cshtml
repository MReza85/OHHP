@model OHHP.Models.Patient
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Membership type: @Model.MembershipType.Name</li>
    <li>Date of Birth: @Model.Birthdate.ToShortDateString()</li>
</ul>

<div class="row">
    <div class="col-md-4">
     <h4>@Model.Name Journal</h4>
        <p style="font-size: 16px">
        @Model.Journal
        </p>
    </div>
</div>
